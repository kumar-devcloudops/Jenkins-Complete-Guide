pipeline {
    agent any
    parameters {
        string(name: 'BRANCH_NAME', defaultValue: 'main', description: 'Branch to build')
        string(name: 'DEPLOY_ENV', defaultValue: 'dev', description: 'Deployment environment')
    }
    stages {
        stage ('checkout') {
            step {
                echo "checking out branch: ${params.BRANCH_NAME} "
            }
        }
    }

}